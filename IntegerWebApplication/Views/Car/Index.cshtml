@*@model List<Car>*@

@*@model Car[]*@

@{
    ViewData["Title"] = "Cars";
}
<div class="card shadow p-3">
    <h1>@ViewData["Title"]</h1>
    <table class="table-bordered table-sm table-striped">
        <thead>
            <tr> 
                <th>IDRegistration</th>
                <th>Type</th>
                <th>Brand</th>
                <th>Varian</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in ViewBag.Cars)
            {
                <tr>
                    <td>@item.IDRegistration</td>
                    <td>@item.Type</td>
                    <td>@item.Brand</td>
                    <td>@item.Varian</td>
                </tr>
            }
        </tbody>
    </table>

